record LinkedList generics T
	Node T root

record Node generics T
	T data
	Node T next

define add(var linkedlist: LinkedList T; value: T) generics T
variables node : Node T
	createNode var node, value
	node.next := linkedlist.node
	linkedlist.node := node

record AssociativeList generics K, V
	K key
	V value
	AssociativeList K, V next

define add(var root: AssociativeList K, V; node : AssociativeList K, V) generics K, V
	node.next := root.next
	root.next := node
