{
  "name": "tree-sitter-shank",
  "version": "0.0.1",
  "description": "shank grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parsing",
    "incremental"
  ],
  "dependencies": {
    "nan": "^2.19.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.21.0"
  },
  "scripts": {
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.shank",
      "file-types": [
        "shank"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "tags": [
        "queries/tags.scm"
      ]
    }
  ]
}
